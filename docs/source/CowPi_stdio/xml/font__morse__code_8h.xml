        <type>uint8_t *</type>
        <definition>uint8_t * cowpi_font_ascii_to_morse_code</definition>
        <argsstring>(uint8_t *destination, char c)</argsstring>
        <name>cowpi_font_ascii_to_morse_code</name>
        <param>
          <type>uint8_t *</type>
